defaults: &defaults
  adapter: "mysql2"
  database: "database"
  username: "username"
  password: "password"
  host: "localhost"

development:
  <<: *defaults

test:
  <<: *defaults

production:
  <<: *defaults
  database: "prod-database"
  username: "prod-username"
  password: "prod-password"
  host: "localhost"